    "build": "babel --presets es2015,stage-0 ./lib/jsx/application.jsx -o ./javascripts/application.js"
  },
  "dependencies": {
    "ejs": "2.4.1",
    "express": "4.13.3"
  },
  "repository": {
    "type": "git",
    "url": ""
