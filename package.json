{
  "name": "tc-whitecube",
  "version": "0.1.0",
  "description": "Whitecubes with Node.js and Express 4",
  "engines": {
    "node": "5.9.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "ava": "npm run build && ava --verbose || true",
    "lint": "./node_modules/.bin/eslint ./*.jsx || true",
